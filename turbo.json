{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "dev": {
      "cache": false
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "test": {
      "outputs": [],
      "cache": false
    },
    "generate": {
      "outputs": ["packages/db/generated/**"]
    },
    "migrate:dev": {
      "cache": false
    },
    "studio": {
      "cache": false
    },
    "format": {
      "outputs": []
    }
  }
}
