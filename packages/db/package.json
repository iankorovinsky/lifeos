{
  "name": "@lifeos/db",
  "version": "0.1.0",
  "private": true,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts"
  },
  "scripts": {
    "generate": "dotenv -e ../../.env -- prisma generate --schema=./prisma/schema",
    "migrate:dev": "dotenv -e ../../.env -- prisma migrate dev --schema=./prisma/schema",
    "migrate:deploy": "dotenv -e ../../.env -- prisma migrate deploy --schema=./prisma/schema",
    "studio": "dotenv -e ../../.env -- prisma studio --schema=./prisma/schema",
    "format": "dotenv -e ../../.env -- prisma format --schema=./prisma/schema"
  },
  "dependencies": {
    "@prisma/client": "^6.0.0"
  },
  "devDependencies": {
    "prisma": "^6.0.0"
  }
}
